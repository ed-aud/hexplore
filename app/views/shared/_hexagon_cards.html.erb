
<% @poi.each do |poi| %>
<div class="card-product">
  <div class="fontawsome-icon-container">
    <i class="fontawsome-icon fa-solid fa-<%=@fontAwsomeIcons[poi.category.strip.to_sym]%>"></i>
  </div>
  <div class="card-product-infos">
    <h5><%= poi.name %></h5>
    <p><%= poi.category.capitalize %></p>
  </div>
</div>
<% end %>
